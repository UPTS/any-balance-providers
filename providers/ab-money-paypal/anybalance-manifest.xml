<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id major_version="3.0" version="17">ab-money-paypal</id>
	<api flags="no_browser"/>
	<name>PayPal</name>
	<description html="true"><![CDATA[
	<p>Retrieves balance on your PayPal account.</p>
	]]></description>
	<author>Dmitry Kochin &lt;dco@mail.ru&gt;</author>
	<depends>
		<module id="library"/>
		<module id="mobile/imei"/>
		<module id="crypt/md5"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<history>history.xml</history>
		<js>main.js</js>
	</files>
	<counters>
		<counter id="balance" name="Balance" units=" USD"/>
		<counter id="balance_rub" name="Balance RUB" units=" RUB"/>
		<counter id="balance_eur" name="Balance EUR" units=" EUR"/>
		<counter id="balance_sek" name="Balance SEK" units=" SEK"/>
	</counters>
	<keywords>пейпал</keywords>
	<type>money, wallet</type>
</provider>